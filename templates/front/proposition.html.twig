</head>
<body>

<!-- PRE LOADER -->


<div class="preloader">
    <div class="spinner">
        <span class="sk-inner-circle"></span>
    </div>
</div>

<style type="text/css">

    @media(min-width:768px) {
        .custom-navbar {
            background: #4dc47d;
        }
        .custom-navbar.top-nav-collapse {
            background: #4dc47d;
            padding: 20px 0;
        }
    }




</style>
<!-- MENU -->
<div class="navbar custom-navbar navbar-fixed-top" role="navigation">
    <div class="container">

        <!-- NAVBAR HEADER -->
        <div class="navbar-header">
            <button class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon icon-bar"></span>
                <span class="icon icon-bar"></span>
                <span class="icon icon-bar"></span>
            </button>
            <!-- lOGO -->
            <a href="/accueil" class="navbar-brand">Campi</a>
        </div>

        <!-- MENU LINKS -->
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/programmesf" class="smoothScroll">{% trans %}Programs{% endtrans %}</a></li>
                <li><a href="/evenementf" class="smoothScroll">{% trans %}Events{% endtrans %}</a></li>
                <li><a href="/accueil#aboutlocaux" class="smoothScroll">{% trans %}Local{% endtrans %}</a></li>
                <li><a href="/accueil#team" class="smoothScroll">{% trans %}Categories{% endtrans %}</a></li>
                <li><a href="/alertsf" class="smoothScroll">{% trans %}Alert{% endtrans %}</a></li>
                <li><a href="/accueil#alerts" class="smoothScroll">{% trans %}Claims{% endtrans %}</a></li>
                <li><a href="/proposition" class="smoothScroll">{% trans %}Propositions{% endtrans %}</a></li>
                <li><a href="/panier" class="smoothScroll">{% trans %}Panier{% endtrans %}</a></li>
                <li><a href="/logoutf" class="smoothScroll">{% trans %}Log Out{% endtrans %}</a></li>




            </ul>
            <ul class="nav navbar-nav ml-auto">
                {% for locale in locales %}
                    {% if locale != app.request.locale %}
                <li><a class="nav-link" href="{{ path('change_locale', {'locale': locale}) }}"><img style="height: 30px; width: 30px" src="{{ asset('images/'~locale~'.png') }}" alt="{{ locale }}">
                    </a></li>
                    {% endif %}
                 {% endfor %}
            </ul>
        </div>

    </div>
</div>






<!-- CONTACT -->
<section id="alerts" class="parallax-section" >
    <div class="container">
        <div class="row">

            <div class="col-md-offset-3 col-md-6 col-sm-12">
                <div class="section-title">

                </div>
            </div>

            <div class="clearfix"></div>
            <div class="col-md-offset-2 col-md-8 col-sm-12">

                <div class="section-title">
                            <h3>{% trans %} Suggest an event{% endtrans %}</h3>
                    <h4>{% trans %}you can propose an event here{% endtrans %}</h4>
                    <h4>{% trans %}it gives us great pleasure{% endtrans %}</h4>
                </div>

                <!-- CONTACT FORM HERE -->
                {{ form_start(form) }}
                <!-- IF MAIL SENT SUCCESSFULLY -->
                <h6 class="text-success">Your message has been sent successfully. </h6>

                <!-- IF MAIL SENDING UNSUCCESSFULL -->
                <h6 class="text-danger">E-mail must be valid and message must be longer than 1 character.</h6>

                <div class="col-md-12 col-sm-12">
                    {#  <input type="text" name="type" class="form-control" id="cf-type" placeholder="Type"> #}
                    {{ form_widget(form.nom,{'attr': {'class': 'form-control','placeholder':'Nom Proposé'}}) }}
                </div>

                <div class="col-md-4 col-sm-4" style="margin-left: 250px" >
                    {#  <input type="text" name="adresse" class="form-control" id="cf-localisation" placeholder="adresse"> #}
                    {{ form_widget(form.date,{'attr': {'class': 'form-control','placeholder':'Date'}}) }}
                </div>

                <div class="col-md-12 col-sm-12">
                    {#  <input type="text" name="localisation" class="form-control" id="inputFirstName" placeholder="Localisation">#}
                    {{ form_widget(form.nombrePlace,{'attr': {'class': 'form-control','placeholder':'Nombre de Place Proposés'}}) }}
                </div>
                <div class="col-md-12 col-sm-12">
                    {#  <input type="text" name="localisation" class="form-control" id="inputFirstName" placeholder="Localisation">#}
                    {{ form_widget(form.mail,{'attr': {'class': 'form-control','placeholder':'Email'}}) }}
                </div>
                <div class="col-md-offset-4 col-md-4 col-sm-offset-4 col-sm-4">
                    <div class="section-btn">
                        <button type="submit" class="form-control" id="cf-submit" name="submit"><span data-hover="Send Message"> {% trans %}<h3>Send Message</h3>{% endtrans %}</span></button>
                    </div>
                </div>

            </div>
            {{ form_end(form) }}

            <div class="col-md-offset-4 col-md-4 col-sm-offset-4 col-sm-4">
                <div class="section-btn">
               <a href="/evenementf"> <button  type="submit" id="cf-submit"  name="return"><span data-hover="Return"> {% trans %}<h3>Return</h3>{% endtrans %}</span></button></a>
                </div>
            </div>
        </div>
    </div>
</section>

